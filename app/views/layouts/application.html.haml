!!!5
%html
  %head
    %title= page_title
    = stylesheet_link_tag 'application', media: 'all'
    = javascript_include_tag 'application'
    = csrf_meta_tags
  %body
    = render 'shared/navigation'
    = link_to "Logout", session_path(session[:user_id]), method: :delete if session[:user_id]
    .container
      = render 'layouts/header'

      = yield
